Points: {{model.points}}

<div class="row">
  <div class="col-md-12">
    <div class='row'>
      <div class='col-md-9'>
         {{outlet}}
      </div>
      <div class='col-md-3'>
        {{#if model.participants.length}}

          <div id="topscore">
            <h3>TOP</h3>
            <hr >
            {{#each user in model.participants}}
              <div class="row">
                <div class="user col-md-12">
                  <table>
                    <tr>
                      <td width="64">
                        <img class="avatar img-circle img-thumbnail" src="http://snipplicious.com/images/guest.png" width="64" alt="Generic placeholder image">
                      </td>
                      <td class="name">
                        {{user.name}}<br />
                      </td>
                      <td class="points">
                        {{user.points}}<br />
                      </td>
                    </tr>
                  </table>
                </div>
              </div>
            {{/each}}
          </div>
        {{/if}}
      </div>
    </div>
  </div>
</div>
